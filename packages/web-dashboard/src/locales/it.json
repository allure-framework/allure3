{
  "statuses": {
    "passed": "Superato",
    "failed": "Fallito",
    "broken": "Rotto",
    "skipped": "Saltato",
    "unknown": "Sconosciuto",
    "total": "Totale"
  },
  "empty": {
    "no-results": "Nessun risultato",
    "no-history-results": "Nessuna informazione storica disponibile"
  },
  "severity": {
    "blocker": "Bloccante",
